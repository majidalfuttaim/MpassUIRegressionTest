{
  "name": "mpass-automation-fe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf ./cypress/reports/* && rimraf ./cypress/output.html",
    "cypress-prod-test": "npm run pretest && npm run cypress-test || npm run posttest",
    "cypress-dev-test": "npm run pretest && npm run cypress-testForDev || npm run posttest",
    "cypress-test": "cypress run --config-file cypress.config.ts",
    "cypress-testForDev": "cypress run --config-file cypress.config.dev.ts",
    "posttest": "npm run merge-reports && npm run generate-htmlreports",
    "merge-reports": "npx mochawesome-merge ./cypress/reports/*.json > ./cypress/output.json",
    "generate-htmlreports": "npx marge ./cypress/output.json --reportDir ./cypress",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress-mochawesome-reporter": "^3.8.2",
    "dotenv": "^17.2.3",
    "googleapis": "^161.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "cypress": "^13.17.0"
  }
}
